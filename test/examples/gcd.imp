function is_even (a : integer) return boolean is
begin
  return a % 2 == 0;
end is_even;

function gcd (a, b : integer) return integer is
  var d : integer;
begin
  d = 1;

  while is_even (a) and is_even(b) loop
    a = a / 2;
    b = b / 2;
    d = d * 2;
  end loop;

  while a # b loop
    if is_even (a) then
      a = a / 2;
    elsif is_even (b) then
      b = b / 2;
    elsif a > b then
      a = (a - b) / 2;
    else
      b = (b - a) / 2;
    end if;
  end loop;

  return a * d;
end gcd;

procedure main () is
   var a, b, c : integer;
begin
   output ("Firsr number: "); input (a);
   output ("Second number: "); input (b);
   c = gcd (a, b);
   output (c); newline;
end main;
